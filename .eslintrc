{
  "parser": "babel-eslint",
  "extends": "airbnb",
  "env": {
    "browser": true,
    "jest": true
  },
  "plugins": [
    "react-hooks"
  ],
    "rules": {
    "no-underscore-dangle": 0,
    "no-nested-ternary": 0,
    "react/forbid-prop-types": 0,
    "class-methods-use-this": 0,
    "function-paren-newline": 0,
    "comma-dangle": [2, "never"],
    "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
    "react-hooks/rules-of-hooks": "error"
  }
}
